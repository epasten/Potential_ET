            bch_clip_makkink_near = bch_file_makkink[(year_noleap >= 2011) & (year_noleap <= 2040)]
            bch_clip_oudin_near = bch_file_oudin[(year_noleap >= 2011) & (year_noleap <= 2040)]
            bch_clip_penman_near = bch_file_penman[(year_noleap >= 2011) & (year_noleap <= 2040)]
