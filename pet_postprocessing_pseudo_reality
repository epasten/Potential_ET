            bch_clip_har_sam_mid = bch_file_har_sam[(year_noleap >= 2041) & (year_noleap <= 2070)]
            bch_clip_hargreaves_mid = bch_file_hargreaves[(year_noleap >= 2041) & (year_noleap <= 2070)]
            bch_clip_jen_hai_mid = bch_file_jen_hai[(year_noleap >= 2041) & (year_noleap <= 2070)]
            bch_clip_makkink_mid = bch_file_makkink[(year_noleap >= 2041) & (year_noleap <= 2070)]
            bch_clip_oudin_mid = bch_file_oudin[(year_noleap >= 2041) & (year_noleap <= 2070)]
            bch_clip_penman_mid = bch_file_penman[(year_noleap >= 2041) & (year_noleap <= 2070)]
            bch_clip_penman_monteith_mid = bch_file_penman_monteith[(year_noleap >= 2041) & (year_noleap <= 2070)]
            bch_clip_pri_tay_mid = bch_file_pri_tay[(year_noleap >= 2041) & (year_noleap <= 2070)]
            bch_clip_turc_mid = bch_file_turc[(year_noleap >= 2041) & (year_noleap <= 2070)]
            bch_year_noleap_clip_mid = year_noleap[(year_noleap >= 2041) & (year_noleap <= 2070)]
            len_clip_mid=len(bch_clip_hamon_mid)
            
            #Far
            bch_clip_hamon_far = bch_file_hamon[(year_noleap >= 2071) & (year_noleap <= 2100)]  
            bch_clip_har_sam_far = bch_file_har_sam[(year_noleap >= 2071) & (year_noleap <= 2100)]
            bch_clip_hargreaves_far = bch_file_hargreaves[(year_noleap >= 2071) & (year_noleap <= 2100)]
            bch_clip_jen_hai_far = bch_file_jen_hai[(year_noleap >= 2071) & (year_noleap <= 2100)]
            bch_clip_makkink_far = bch_file_makkink[(year_noleap >= 2071) & (year_noleap <= 2100)]
