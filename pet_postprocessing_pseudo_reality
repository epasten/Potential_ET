            bch_daily_clip_ts_oudin_ref = np.zeros([len_clip_ref])
            bch_daily_clip_ts_penman_ref = np.zeros([len_clip_ref])
            bch_daily_clip_ts_penman_monteith_ref = np.zeros([len_clip_ref])
            bch_daily_clip_ts_pri_tay_ref = np.zeros([len_clip_ref])
            bch_daily_clip_ts_turc_ref = np.zeros([len_clip_ref])
            
            #Near
            bch_daily_clip_ts_hamon_near = np.zeros([len_clip_near])
            bch_daily_clip_ts_har_sam_near = np.zeros([len_clip_near])
            bch_daily_clip_ts_hargreaves_near = np.zeros([len_clip_near])
