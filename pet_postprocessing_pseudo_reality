            bch_clip_pri_tay_near = bch_file_pri_tay[(year_noleap >= 2011) & (year_noleap <= 2040)]
            bch_clip_turc_near = bch_file_turc[(year_noleap >= 2011) & (year_noleap <= 2040)]
