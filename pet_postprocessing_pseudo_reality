            pet_summer_ref_sum_all_cms_makkink[yy-1971,cm_i] = np.sum(df_cm.Makkink)
            pet_summer_ref_sum_all_cms_oudin[yy-1971,cm_i] = np.sum(df_cm.Oudin)
