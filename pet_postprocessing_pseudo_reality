            bch_daily_clip_ts_penman_near = np.zeros([len_clip_near])
            bch_daily_clip_ts_penman_monteith_near = np.zeros([len_clip_near])
            bch_daily_clip_ts_pri_tay_near = np.zeros([len_clip_near])
            bch_daily_clip_ts_turc_near = np.zeros([len_clip_near])
            
