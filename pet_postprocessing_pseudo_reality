            pet_summer_ref_sum_all_cms_penman_montheit[yy-1971,cm_i] = np.sum(df_cm.Penman_Montheit)
            pet_summer_ref_sum_all_cms_pri_tay[yy-1971,cm_i] = np.sum(df_cm.Priestly_Taylor)
